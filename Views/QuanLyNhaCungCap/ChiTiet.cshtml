@model WebBanHang.Models.NhaCungCap
@{
    ViewBag.Title = "ChiTiet";
    Layout = "~/Views/Layout/AdminLayout.cshtml";
}
<div class="col-md-12">
    <div class="panel panel-primary">
        <div class="panel-heading">THÔNG TIN CHI TIẾT NHÀ CUNG CẤP  </div>
        <div class="panel-body">
            <!--row-->
            <div class="row" style="margin-top:5px;">
                <div class="col-md-2">Mã Nhà Cung Cấp</div>
                <div class="col-md-10">
                    @Model.MaNCC
                </div>
            </div>
            <!--end row-->
            <!--row-->
            <div class="row" style="margin-top:5px;">
                <div class="col-md-2">Tên Nhà Cung Cấp</div>
                <div class="col-md-10">
                    @Model.TenNCC
                </div>
            </div>
            <!--end row-->
            <!--row-->
            <div class="row" style="margin-top:5px;">
                <div class="col-md-2">Địa Chỉ Nhà Cung Cấp</div>
                <div class="col-md-10">
                    @Model.DiaChi
                </div>
            </div>
            <!--end row-->
            <!--row-->
            <div class="row" style="margin-top:5px;">
                <div class="col-md-2">Số Điện Thoại Liên Hệ</div>
                <div class="col-md-10">
                    @Model.SoDienThoai
                </div>
            </div>
            <!--end row-->
            <!--row-->
            <div class="row" style="margin-top:5px;">
                <div class="col-md-2">Số Fax</div>
                <div class="col-md-10">
                    @Model.Fax
                </div>
            </div>
            <!--end row-->
            <!--row-->
        </div>
        <!-- Modal footer -->
        <div class="modal-footer">
            @Html.ActionLink("Close", "Index", "QuanLyNhaCungCap", new { @class = "btn btn-default" })
        </div>
    </div>
</div>
